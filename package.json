{
  "name": "gatsby-starter-chakra-story",
  "version": "1.0.0",
  "private": true,
  "description": "It is a starter of GatsbyJS that incorporates chakra-ui and storybook.",
  "author": "koichi.tachizaki",
  "keywords": [
    "gatsby",
    "gatsby-starter",
    "chakra-ui",
    "storybook",
    "typescript"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx,json,html,md}\"",
    "fix": "yarn typecheck && yarn lint --fix && yarn format",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@chakra-ui/gatsby-plugin": "^3.0.0",
    "@chakra-ui/react": "^2.1.2",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "dayjs": "^1.11.2",
    "framer-motion": "^6.3.10",
    "gatsby": "^4.15.2",
    "gatsby-plugin-image": "^2.14.1",
    "gatsby-plugin-manifest": "^4.14.0",
    "gatsby-plugin-next-seo": "^1.10.0",
    "gatsby-plugin-react-helmet": "^5.14.0",
    "gatsby-plugin-sharp": "^4.14.1",
    "gatsby-plugin-sitemap": "^5.14.0",
    "gatsby-source-filesystem": "^4.14.0",
    "gatsby-source-microcms": "^2.0.0",
    "gatsby-transformer-sharp": "^4.14.0",
    "mvp.css": "^1.8.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^1.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@storybook/addon-actions": "^6.5.6",
    "@storybook/addon-essentials": "^6.5.6",
    "@storybook/addon-interactions": "^6.5.6",
    "@storybook/addon-links": "^6.5.6",
    "@storybook/builder-webpack5": "^6.5.6",
    "@storybook/manager-webpack5": "^6.5.6",
    "@storybook/react": "^6.5.6",
    "@storybook/testing-library": "0.0.11",
    "@types/node": "^17.0.31",
    "@types/react": "^17.0.44",
    "@types/react-dom": "^17.0.16",
    "@types/react-imgix": "^9.4.0",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "babel-loader": "^8.2.5",
    "eslint": "^8.15.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-storybook": "^0.5.12",
    "gatsby-plugin-pnpm": "^1.2.10",
    "prettier": "^2.6.2",
    "storybook-addon-gatsby": "^0.0.5",
    "typescript": "^4.6.4"
  }
}
